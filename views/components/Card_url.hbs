<article class='card mb-3'>
  <div class='card-body'>
    <a href='{{url.originURL}}' target='_blank' class='nav-link fs-5'>
      {{url.originURL}}
    </a>
    <a
      href='http://localhost:5000/{{url.shortURL}}'
      target='_blank'
      class='nav-link fs-4'
    >
      http://localhost:5000/{{url.shortURL}}
    </a>

    <div class='mt-3'>
      <a href='/eliminar/{{url._id}}' class='btn btn-danger'>Eliminar</a>
      <a href='/editar/{{url._id}}' class='btn btn-warning'>Editar</a>
      <button
        type='button'
        class='btn btn-info'
        data-shortUrl='{{url.shortURL}}'
      >Copiar</button>
    </div>
  </div>
</article>